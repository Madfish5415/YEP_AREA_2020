{
  "name": "area-mobile",
  "version": "1.0.0",
  "description": "Mobile client for AREA",
  "license": "MIT",
  "main": ".expo/__generated__/AppEntry.js",
  "scripts": {
    "build:export": "expo export --dev --public-url http://0.0.0.0:5000 --output-dir build/",
    "build:serve": "serve build/",
    "build:android-setup": "turtle setup:android",
    "build:android": "turtle build:android --public-url http://0.0.0.0:5000/android-index.json --allow-non-https-public-url",
    "build:ios-setup": "turtle setup:ios",
    "build:ios": "turtle build:ios --public-url http://0.0.0.0:5000/android-index.json --allow-non-https-public-url",
    "start": "expo start",
    "start:android": "expo start --android",
    "start:ios": "expo start --ios",
    "clean": "rm -rf .expo/ build/",
    "fclean": "yarn clean && rm -rf node_modules/",
    "postinstall": "expo-yarn-workspaces postinstall"
  },
  "dependencies": {
    "@area-common/blocs": "*",
    "@area-common/styles": "*",
    "@felangel/react-bloc": "^0.3.0",
    "expo": "^40.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-native": "^0.63.4",
    "react-native-paper": "^4.6.0",
    "react-native-unimodules": "^0.12.0",
    "react-native-vector-icons": "^7.1.0",
    "react-native-web": "^0.14.10"
  },
  "devDependencies": {
    "@area-common/types": "*",
    "@babel/core": "^7.0.0",
    "@expo/metro-config": "^0.1.49",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-native": "^0.63.45",
    "expo-yarn-workspaces": "^1.3.0",
    "serve": "^11.3.2",
    "turtle-cli": "^0.20.5"
  }
}
