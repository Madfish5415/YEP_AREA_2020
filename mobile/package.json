{
  "name": "area-mobile",
  "version": "1.0.0",
  "description": "Mobile client for AREA",
  "license": "MIT",
  "main": "build/index.js",
  "scripts": {
    "build:android": "cd android/ && ./gradlew build",
    "build:ios": "cd android/ && xcodebuild build",
    "start": "react-native start",
    "start:android": "react-native run-android",
    "start:ios": "react-native run-ios",
    "clean": "rm -rf build/",
    "clean:android": "cd android/ && ./gradlew clean",
    "clean:ios": "cd ios/ && xcodebuild clean",
    "fclean": "yarn clean && rm -rf node_modules/",
    "postinstall": "node postinstall.js"
  },
  "dependencies": {
    "@area-common/blocs": "*",
    "@area-common/styles": "*",
    "@felangel/react-bloc": "^0.3.0",
    "react": "^17.0.1",
    "react-native": "^0.63.4",
    "react-native-paper": "^4.6.0",
    "react-native-vector-icons": "^7.1.0"
  },
  "devDependencies": {
    "@area-common/types": "*",
    "@babel/core": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@types/react": "^17.0.0",
    "@types/react-native": "^0.63.45",
    "metro-react-native-babel-preset": "^0.64.0"
  }
}
